   "source": [
    "def sum_of_digits(n):\n",
    "    last_digit = 0\n",
    "    n = abs(n)\n",
    "    while n > 0:\n",
    "        last_digit = n % 10\n",
    "        n // = 10\n",
    "        last_digit += last_digit\n",
    "    return last_digit"
   ]
  },
